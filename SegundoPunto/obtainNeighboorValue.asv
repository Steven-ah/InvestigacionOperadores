function obtainNeighboorValue(a,b,c,d,index)
      global adM;
      global A;
      indexA=find(A==A(a,b));
      indexB=find(A==A(c,d));
      if length(indexA) > 1
           indexA = indexA(indexA>=index);
           indexA = indexA(1);
      end
      adM(indexA, indexB)=abs(A(a,b) - A(c,d));
      adM(indexB, indexA)=adM(indexA, indexB);
end